{
  "app": { "basePath": "/api" },
  "server": {
    "http": { "enabled": true, "port": 3000 },
    "https": { "enabled": false, "port": 3443, "redirectAllHttpToHttps": false },
    "trustProxy": false
  },
  "db": { "uri": "mongodb://localhost:27017/testTP" },
  "security": {
    "jwt": { "expiresIn": "1h" },
    "cors": { "origins": ["http://localhost:4200", "http://localhost:3000"] },
    "rateLimit": { "windowMs": 900000, "max": 100 }
  }
}
