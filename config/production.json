{
  "server": {
    "http": { "enabled": true, "port": 3000 },
    "https": { "enabled": true, "port": 3443, "redirectAllHttpToHttps": true },
    "trustProxy": true
  },
  "db": { "uri": "mongodb://localhost:27017/media_prod" },
  "security": {
    "jwt": { "expiresIn": "1d" },
    "cors": { "origins": ["https://frontend.example.com"] },
    "rateLimit": { "windowMs": 900000, "max": 200 }
  }
}
